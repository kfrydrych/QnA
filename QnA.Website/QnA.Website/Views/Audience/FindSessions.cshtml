@model QnA.Application.Audience.Queries.FindSessions.FindSessionsResult

@{
    ViewData["Title"] = "Sessions";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h4>Available Sessions</h4>

<div class="list-group">
    @foreach (var item in Model.Sessions)
    {
        <a asp-action="Session" asp-route-id="@item.Id" class="list-group-item list-group-item-action">@item.Title</a>
    }
</div>




